<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Unidad_2.Views.MaestroDetalle.MasterDetail"
             NavigationPage.HasNavigationBar="False"
             xmlns:pages="clr-namespace:Unidad_2.Views.MaestroDetalle">
  <FlyoutPage.Flyout>
    <pages:MasterDetailFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:MasterDetailDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>